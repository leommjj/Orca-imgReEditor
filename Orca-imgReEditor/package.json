  },
  "devDependencies": {
    "@types/date-fns": "^2.5.3",
    "@types/node": "^24.6.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-external-globals": "^0.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
