{
  "name": "orca-plugin-template",
  "version": "2.7.2",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "icon.png",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
